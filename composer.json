{
  "name": "commercepundittech/laravel-seeding",
  "description": "Database seeder for Laravel",
  "keywords": [
    "laravel",
    "database",
    "seeder"
  ],
  "homepage": "https://www.commercepundit.com/",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Vallabh Kansagara",
      "email": "vallabh.kansagara@commercepundit.com",
      "homepage": "https://vrkansagara.in/"
    }
  ],
  "require": {
    "php" : ">=7.0"
  },
  "autoload": {
    "psr-4": {
      "CommercePunditTech\\LaravelSeeding\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "support": {
    "email": "vallabh.kansagara@commercepundit.com",
    "issues": "https://github.com/commercepundittech/laravel-seeding/issues",
    "source": "https://github.com/commercepundittech/laravel-seeding"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "branch-alias": {
        "dev-master": "development"
      },
      "providers": [
        "CommercePunditTech\\LaravelSeedingServiceProvider"
      ],
      "aliases": {
        "LaravelSeeding": "CommercePunditTech\\LaravelSeeding\\Facades"
      }
    }
  },
  "scripts": {
    "test": "phpunit",
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
    ]
  }
}
